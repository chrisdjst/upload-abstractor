{
    "name": "chrisdjst/upload-abstractor",
    "description": "Abstração SOLID para upload com suporte a S3 e armazenamento local.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Steffens Theilacher",
            "email": "christophertheilacher@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "aws/aws-sdk-php": "^3.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "UploadAbstractor\\": "src/UploadAbstractor/",
            "EnvManager\\": "src/EnvManager/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}